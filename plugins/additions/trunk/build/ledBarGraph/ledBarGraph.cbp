<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="wxLedBarGraph (Unix)" />
		<Option pch_mode="0" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="Shared Library Release">
				<Option output="../../lib/lib`wx-config --basename`_ledbargraph-`wx-config --release`.so" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DWXMAKINGDLL_WXLEDBARGRAPH" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DMONOLITHIC" />
				</Compiler>
				<Environment>
					<Variable name="DEBUG" value="no" />
				</Environment>
			</Target>
			<Target title="Shared Library Debug">
				<Option output="../../lib/lib`wx-config --debug=yes --basename`_ledbargraph-`wx-config --release`.so" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objsd" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DWXMAKINGDLL_WXLEDBARGRAPH" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DMONOLITHIC" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Environment>
					<Variable name="DEBUG" value="yes" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Shared Library Release;Shared Library Debug;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`wx-config --debug=$(DEBUG) --cflags`" />
			<Add directory="../../include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --debug=$(DEBUG) --libs`" />
		</Linker>
		<Unit filename="../../include/wx/ledBarGraph/ledBarGraph.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="Shared Library Release" />
			<Option target="Shared Library Debug" />
		</Unit>
		<Unit filename="../../src/ledBarGraph/ledBarGraph.cpp">
			<Option compilerVar="CPP" />
			<Option target="Shared Library Release" />
			<Option target="Shared Library Debug" />
		</Unit>
		<Extensions>
			<code_completion />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
